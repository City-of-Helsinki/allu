{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": false,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "noEmitHelpers": true,
    "pretty": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noFallthroughCasesInSwitch": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "core-js",
      "jasmine"
    ]
  },
  "exclude": [
    "node_modules",
    "typings/main.d.ts",
    "typings/main"
  ],
  "filesGlob": [
    "./src/**/*.ts",
    "./test/**/*.ts",
    "!./node_modules/**/*.ts",
    "./src/js/leaflet/path-transform/index.js",
    "./src/js/leaflet/draw-transform/index.js",
    "./src/js/leaflet/draw-intersect/index.js",
    "./src/js/leaflet/circle-to-poly/index.js",
    "src/custom-typings.d.ts",
    "typings/browser.d.ts",
    "src/typings/leaflet-proj/index.d.ts",
    "src/typings/file-saver/index.d.ts",
    "src/typings/leaflet-draw-drag/index.d.ts",
    "src/typings/leaflet-groupedlayercontrol/index.d.ts",
    "src/typings/leaflet-measure-path/index.d.ts",
    "src/typings/leaflet-translations/index.d.ts",
    "src/typings/finnish-ssn/index.d.ts",
    "src/typings/leaflet-intersect/index.d.ts"
  ],
  "awesomeTypescriptLoaderOptions": {
    "resolveGlobs": true,
    "forkChecker": true
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  }
}
