{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "allu"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "../test/test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "../test/tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "assets/main.scss",
        "assets/materialize/materialize.css",
        "../node_modules/leaflet/dist/leaflet.css",
        "../node_modules/leaflet-draw/dist/leaflet.draw.css",
        "../node_modules/leaflet-groupedlayercontrol/dist/leaflet.groupedlayercontrol.min.css",
        "../node_modules/leaflet-measure-path/leaflet-measure-path.css",
        "../node_modules/leaflet.markercluster/dist/MarkerCluster.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/hammerjs/hammer.js",
        "../node_modules/materialize-css/dist/js/materialize.js",
        "../node_modules/file-saver/FileSaver.js",
        "../node_modules/finnish-ssn/dist/finnish-ssn.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {},
    "build": {
      "baseHref": "/"
    },
    "serve": {
      "host": "localhost",
      "port": "3000"
    }
  }
}
